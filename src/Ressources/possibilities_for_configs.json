{
    "applicationSettings": {
        "logfile": "<filename>",
        "createLogs": false,
        "logfileRandomPrefix": true,
        "showImages": true,
        "saveImages": false
    },

    "imageStream": [
        {
          "type": "picam",
          "shutterTime": 4000,
          "resolutionW": 640,
          "resolutionH": 480
        },
        {
          "type": "file",
          "path": "imageSeries/shutter4/",
          "framerate": 10
        }
    ],

    "imageProcessors": [
        {
            "id": "1",
            "type": "numberDetection",
            "preProcessing": [
                  {
                    "type": "none"
                  },
                  {
                    "type": "gammaCorrection",
                    "filter": "5.5"
                  }
            ],
            "segmentation": [
              {
                "type": "canny",
                "threshold1": "250",
                "threshold2": "50"
              },
              {
                "type": "threshold",
                "threshold1": 200,
                "threshold2": 150,
                "threshMode": ["binary","otus","binOtsu"]
              }
            ],
            "featureDetection": {
                "type": "findContours"
            },
            "signDetection": {
                "type": "DnnMnist",
                "path": "model/mnist_trained_model.h5"
            }
        },
	{
	    "id": "2",
	    "type": "numberDetection",
	    "preProcessing": {
		"type": "gammaCorrection",
		"filter": "5.5"
	    },
	    "segmentation": {
		"type": "canny",
		"minThreshold": "250",
		"maxThreshold": "100"
	    },
	    "featureDetection": {
		"type": "findContours"
	    },
	    "signDetection": {
		"type": "DnnMnist",
		"path": "model/mnist_trained_model.h5"
	    }
	}
    ]
}
