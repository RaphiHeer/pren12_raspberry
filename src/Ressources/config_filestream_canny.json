{
    "applicationSettings": {
        "logfile": "data.log",
        "createLogs": false,
        "logfileRandomPrefix": true,
        "showImages": true,
        "saveImages": false
    },
    "imageStream": {
        "type": "file",
        "path": "imageSeries/shutter4/",
        "framerate": 10
    },

    "imageProcessors": [
        {
            "id": "1",
            "type": "numberDetection",
            "preProcessing": {
                "type": "gammaCorrection",
                "gamma": 1
            },
            "segmentation": {
                "type": "canny",
                "threshold1": 250,
                "threshold2": 50
            },
            "featureDetection": {
                "type": "findContours"
            },
            "signDetection": {
                "type": "DnnMnist",
                "path": "model/mnist_trained_model.h5"
            }
        }
    ]
}