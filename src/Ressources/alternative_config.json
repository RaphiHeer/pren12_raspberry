{
    "logfile": "data.log",
    "createLogs": false,
    "logfileRandomPrefix": true,
    "showImages": true,
    "saveImages": false,

    "imageStream": {
        "type": "files",
        "folderPath": "imageSeries/shutter10"
    },

    "imageProcessors": [
        {
            "id": "1",
            "type": "numberDetection",
            "preprocessing": {
                "type": "gammaCorrection",
                "gamma": 5
            },
            "segmentation": {
                "type": "canny",
                "threshold1": 200,
                "threshold2": 255
            },
            "featureDetection": {
                "type": "findContours"
            },
            "signDetection": {
                "type": "DnnMnist",
                "path": "model/mnist_trained_model.h5"
            }
        },
        {
            "id": "2",
            "type": "startDetection",
            "preprocessing": {
                "type": "blur",
                "filter": "gaussian"
            },
            "segmentation": {
                "type": "canny",
                "minThreshold": 200,
                "maxThreshold": 255
            },
            "featureDetection": {
                "type": "findRectangles"
            },
            "signDetection": {
                "type": "templateMatching",
                "templateFolder": "numberTemplates"
            }

        }
    ]
}