{
    "applicationSettings": {
        "logfile": "data.log",
        "createLogs": false,
        "logfileRandomPrefix": true,
        "showImages": false,
        "saveImages": false,
        "concurrency": "process"
    },

    "imageStream": {
        "type": "picam",
        "framerate": 30,
        "iso": 1600,
        "shutterTime": 1600,
        "resolutionW": 640,
        "resolutionH": 480
    },

    "imageProcessors":
    {
            "id": "1",
            "type": "numberDetection",
            "numberOfImageProcessors": 5,
            "preProcessing": {
                "type": "none",
                "gamma": 1
            },
            "segmentation": {
                "type": "canny",
                "threshold1": 50,
                "threshold2": 80
            },
            "featureDetection": {
                "type": "findContours"
            },
            "signDetection": {
                "type": "TM",
                "path": "model/mnist_cnn_2.h5",
                "savePredictions": true,
                "savePredictionsPath": "ml/"
            }
    }
}
