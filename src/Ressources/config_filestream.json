{
    "applicationSettings": {
        "logfile": "data.log",
        "createLogs": false,
        "logfileRandomPrefix": true,
        "showImages": true,
        "saveImages": false,
        "concurrency": "process"
    },
    "imageStream": {
        "type": "file",
        "path": "imageSeries/final08/",
        "framerate": 10
    },

    "imageProcessors":
    {
        "id": "1",
        "type": "numberDetection",
        "numberOfImageProcessors": 1,
        "preProcessing": {
            "type": "gammaCorrection",
            "gamma": 1
        },
        "segmentation": {
            "type": "canny",
            "threshold1": 50,
            "threshold2": 100
        },
        "featureDetection": {
            "type": "findContours"
        },
        "signDetection": {
            "type": "TM",
            "path": "model/mnist_cnn_2.h5",
            "savePredictions": true,
            "savePredictionsPath": "ml/"
        }
    }
}