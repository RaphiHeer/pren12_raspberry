{
    "applicationSettings": {
        "logfile": "data.log",
        "createLogs": false,
        "logfileRandomPrefix": true,
        "showImages": true,
        "saveImages": false,
        "concurrency": "none"
    },
    "imageStream": {
        "type": "file",
        "path": "imageSeries/led05/",
        "framerate": 10
    },

    "imageProcessors": [
        {
            "id": "1",
            "type": "numberDetection",
            "preProcessing": {
                "type": "gammaCorrection",
                "gamma": 2
            },
            "segmentation": {
                "type": "canny",
                "threshold1": 10,
                "threshold2": 250
            },
            "featureDetection": {
                "type": "findContours"
            },
            "signDetection": {
                "type": "TM",
                "path": "model/mnist_cnn_2.h5",
                "savePredictions": true,
                "savePredictionsPath": "ml/"
            }
        }
    ]
}